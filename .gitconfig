[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow 
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  st = status
  ci = commit
  kick = commit --allow-empty -m \"kick\" #git config --global alias.kick 'commit --allow-empty -m "kick"'
[core]
  editor = subl --wait
[commit]
	gpgsign = true
